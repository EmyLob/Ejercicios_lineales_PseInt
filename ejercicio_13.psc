Algoritmo ejercicio_13
	// 1. Definir variables
	Definir ingresos, gastos, ganancia Como Real;
	
	// 2. Asignación de datos (entrada del usuario)
	Escribir "Cuanto fue el ingreso?";
	Leer ingresos;
	Escribir "Cuanto fue el gasto?";
	Leer gastos;
	
	// 3. Procesar datos (cálculo de la ganancia)
	ganancia <- ingresos - gastos;
	
	// 4. Mostrar resultados
	Escribir "La ganancia neta seria: ", ganancia;
FinAlgoritmo
