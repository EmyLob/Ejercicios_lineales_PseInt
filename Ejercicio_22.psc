Algoritmo Ejercicio_22
	// 1. Definir variables
	Definir paginas, precio, total Como Real;
	
	// 2. Entrada de datos
	Escribir "¿Cuánto cuesta cada página?";
	Leer precio;
	Escribir "¿Cuántas páginas fueron?";
	Leer paginas;
	
	// 3. Proceso (cálculo del total)
	total <- paginas * precio;
	
	// 4. Salida de resultados
	Escribir "El valor de la impresión es de: ", total;
	
FinAlgoritmo
