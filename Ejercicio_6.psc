Algoritmo Ejercicio_6
	// 1. Definir variables
	Definir num1, num2, num3, num4, num5 como Entero;
	Definir Suma como Entero;
	Definir mensaje como Caracter;
	
	// 2. Asignación de datos
	num1 <- 20;
	num2 <- 90;
	num3 <- 4;
	num4 <- 32;
	num5 <- 45;
	
	// 3. Procesar datos (suma de los números)
	Suma <- num1 + num2 + num3 + num4 + num5;
	mensaje <- "El resultado es: " + ConvertirATexto(Suma);
	
	// 4. Mostrar resultados
	Escribir mensaje;
FinAlgoritmo
